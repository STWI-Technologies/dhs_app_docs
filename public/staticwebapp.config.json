{	
  "globalHeaders": {
    "Access-Control-Allow-Origin": "*",
    "Access-Control-Allow-Methods": "POST, GET, OPTIONS"
  },
    "responseOverrides": {
      "404": {
        "rewrite": "/404.html",
        "statusCode": 404
      }
    },
    "routes": [
      {
        "route": "/",
        "redirect": "https://directhomeservice.com",
        "statusCode": 301
      }
    ]
  }
